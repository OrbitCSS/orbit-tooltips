{
  "name": "orbit-tooltips",
  "version": "0.1.0",
  "description": "Orbit Tooltips",
  "main": "tooltips.scss",
  "homepage": "https://orbitcss.com/documentation/extensions/tooltips",
  "repository": {
    "type": "git",
    "url": "git@github.com:OrbitCSS/orbit-tooltips.git"
  },
  "bugs": {
    "url": "https://github.com/OrbitCSS/orbit-tooltips/issues"
  },
  "scripts": {
    "build:sass": "node-sass --output-style expanded tooltips.scss css/tooltips.css",
    "build:clean": "cleancss -o css/tooltips.min.css css/tooltips.css",
    "build": "npm-run-all build:sass build:clean"
  },
  "author": "Rhys Hall <hello@rhyshall.com>",
  "license": "MIT",
  "keywords": [
    "css",
    "scss",
    "sass",
    "orbitcss"
  ],
  "devDependencies": {
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.1.3"
  }
}
